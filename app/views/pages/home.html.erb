<div class="home">
  <section class="slide-show">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
      <!-- Indicators -->
      <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
      </ol>

      <!-- Wrapper for slides -->
      <div class="carousel-inner">
        <div class="item active home_first-slider">
          <h1>Mas huéspedes, Más dinero, Más simple</h1>
          <%= link_to '#contact' do %>
            <h4>Escribenos!</h4>
          <% end %>
          <img src="https://images.pexels.com/photos/709767/pexels-photo-709767.png?auto=compress&cs=tinysrgb&dpr=2&h=750&w=1260" alt="CUARTO DE HOTEL1" class="hidden-md hidden-lg hiden-xl">
        </div>

        <div class="item home_second-slider">
          <h1>Manejamos apartamenos en Cartagena, Santa Marta y Barranquilla</h1>
          <img src="https://images.pexels.com/photos/769585/pexels-photo-769585.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=750&w=1260" alt="CUARTO DE HOTEL2" class="hidden-md hidden-lg hiden-xl">
        </div>

        <div class="item home_third-slider">
          <h1>Administracion y Limpieza de Apartamentos</h1>
          <img src="https://images.pexels.com/photos/533157/pexels-photo-533157.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=750&w=1260" class="hidden-md hidden-lg hiden-xl">
        </div>
      </div>

      <!-- Left and right controls -->
      <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
  </section>
